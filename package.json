{
  "name": "deliveryapp-api",
  "version": "1.0.0",
  "description": "API for DeliveryApp",
  "author": "denlysenko",
  "license": "MIT",
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "nodemon",
    "start:debug": "nodemon --config nodemon-debug.json",
    "prestart:prod": "rimraf dist && tsc && copyfiles -u 1 src/**/.env dist",
    "start:prod": "node dist/main.js",
    "start:hmr": "node dist/server",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "webpack": "webpack --config webpack.config.js"
  },
  "dependencies": {
    "@nestjs/common": "^7.0.7",
    "@nestjs/core": "^7.0.7",
    "@nestjs/mongoose": "^6.4.0",
    "@nestjs/passport": "^7.0.0",
    "@nestjs/platform-express": "^7.0.7",
    "@nestjs/platform-socket.io": "^7.0.7",
    "@nestjs/swagger": "^4.5.1",
    "@nestjs/websockets": "^7.0.7",
    "dotenv": "^8.2.0",
    "firebase-admin": "^8.10.0",
    "helmet": "^3.22.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "module-alias": "^2.2.2",
    "mongoose": "^5.9.7",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "pg": "^8.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.5",
    "sequelize": "^5.21.6",
    "sequelize-typescript": "^1.1.0",
    "socket.io": "^2.3.0",
    "swagger-ui-express": "^4.1.4",
    "typescript": "^3.8.3"
  },
  "devDependencies": {
    "@nestjs/testing": "^7.0.7",
    "@types/express": "^4.17.4",
    "@types/jest": "^25.2.1",
    "@types/lodash": "^4.14.149",
    "@types/mongoose": "^5.7.8",
    "@types/node": "^13.11.0",
    "@types/socket.io": "^2.1.4",
    "@types/supertest": "^2.0.8",
    "copyfiles": "^2.2.0",
    "jest": "^25.3.0",
    "nodemon": "^2.0.2",
    "prettier": "^2.0.4",
    "rimraf": "^3.0.2",
    "sequelize-cli": "^5.5.1",
    "supertest": "^4.0.2",
    "ts-jest": "^25.3.1",
    "ts-loader": "^6.2.2",
    "ts-node": "^8.8.2",
    "tsconfig-paths": "^3.9.0",
    "tslint": "6.1.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "_moduleAliases": {
    "@auth": "dist/auth",
    "@common": "dist/common",
    "@config": "dist/config",
    "@core": "dist/core",
    "@database": "dist/database",
    "@logs": "dist/logs",
    "@messages": "dist/messages",
    "@orders": "dist/orders",
    "@payments": "dist/payments",
    "@settings": "dist/settings",
    "@users": "dist/users"
  }
}
