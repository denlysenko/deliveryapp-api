{
  "name": "deliveryapp-api",
  "version": "0.0.1",
  "description": "API for DeliveryApp",
  "author": "denlysenko",
  "license": "MIT",
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "nodemon",
    "start:debug": "nodemon --config nodemon-debug.json",
    "prestart:prod": "rimraf dist && tsc && copyfiles -u 1 src/**/.env dist",
    "start:prod": "node dist/main.js",
    "start:hmr": "node dist/server",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "webpack": "webpack --config webpack.config.js"
  },
  "dependencies": {
    "@nestjs/common": "^5.1.0",
    "@nestjs/core": "^5.1.0",
    "@nestjs/mongoose": "^5.2.2",
    "@nestjs/passport": "^5.1.0",
    "@nestjs/swagger": "^2.5.1",
    "@nestjs/websockets": "^5.3.8",
    "dotenv": "^6.0.0",
    "helmet": "^3.13.0",
    "jsonwebtoken": "^8.3.0",
    "lodash": "^4.17.11",
    "module-alias": "^2.1.0",
    "mongoose": "^5.2.17",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "pg": "^7.4.3",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.2.2",
    "sequelize": "^4.39.0",
    "sequelize-typescript": "^0.6.6",
    "socket.io": "^2.1.1",
    "typescript": "^3.0.1"
  },
  "devDependencies": {
    "@nestjs/testing": "^5.3.8",
    "@types/express": "^4.16.0",
    "@types/jest": "^23.3.1",
    "@types/lodash": "^4.14.116",
    "@types/mongoose": "^5.2.17",
    "@types/node": "^10.7.1",
    "@types/socket.io": "^1.4.38",
    "@types/supertest": "^2.0.5",
    "copyfiles": "^2.1.0",
    "jest": "^23.5.0",
    "nodemon": "^1.18.3",
    "prettier": "^1.14.2",
    "rimraf": "^2.6.2",
    "supertest": "^3.1.0",
    "ts-jest": "^23.1.3",
    "ts-loader": "^4.4.2",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.5.0",
    "tslint": "5.11.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "_moduleAliases": {
    "auth": "dist/auth",
    "common": "dist/common",
    "config": "dist/config",
    "core": "dist/core",
    "database": "dist/database",
    "logs": "dist/logs",
    "messages": "dist/messages",
    "orders": "dist/orders",
    "payments": "dist/payments",
    "settings": "dist/settings",
    "users": "dist/users"
  }
}
